_global_script_class_icons={
}

[application]

config/name="WhatTheHex"
config/description="What The Hex? is a simple yet addictive puzzle game involving rotating hexagons to align their colors."
run/main_scene="res://root.tscn"
boot_splash/image="res://boarish-logo-only.png"
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
config/name_fr="Qu'est-ce Que L'Hexagone?"

[autoload]

RuntimeConfig="*res://RuntimeConfig.cs"

[debug]

settings/stdout/verbose_stdout=true

[display]

mouse_cursor/custom_image="res://cursor_hand.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[filesystem]

import/fbx/use_fbx=false

[global]

debug=false

